{
  "sections": {
    "main": {
      "type": "main-page",
      "settings": {
        "header_color": "#ffffff",
        "header_bg_color": "#ff5c78",
        "header_bg_color_opacity": 60,
        "header_bg_image": "shopify:\/\/shop_images\/city-shot.jpg",
        "header_padding_top": 70,
        "header_padding_bottom": 70,
        "header_margin_bottom": 30,
        "padding_bottom": 28
      }
    },
    "custom_liquid_JfRVYT": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"yotpo-widget-instance\" data-yotpo-instance-id=\"481658\" data-yotpo-product-id=\"{{product.id}}\" data-yotpo-name=\"{{ product.title | escape }}\" data-yotpo-url=\"{{ shop.url }}{{ product.url }}\" data-yotpo-image-url=\"{{ product.featured_image | product_img_url: \"large\" | prepend: \"https:\" | replace: '?', '%3F' | replace: '&','%26'}}\" data-yotpo-price=\"{{ product.price | divided_by: 100.00 }}\" data-yotpo-currency=\"{{ shop.currency }}\" data-yotpo-description=\"{{ product.description | escape }}\"><\/div>",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
    }
  },
  "order": [
    "main",
    "custom_liquid_JfRVYT"
  ]
}
